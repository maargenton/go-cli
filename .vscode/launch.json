    "version": "0.2.0",
    "configurations": [
        {
            "name": "enumer pkg/strcase",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/enumer",
            "cwd": "${workspaceFolder}/pkg/strcase",
        },
        {
            "name": "sercat-demo",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/sample/sercat-demo",
            // "args": [ "-zzzv" ],
            "args": [
                "--baudrate",
                "230400",
                "--format",
                "8N1",
                "--",
                "--verbose",
                "--timestamp",
                "/dev/tty.Bluetooth-Incoming-Port",
            ]
        },
        {
            "name": "subcmd-demo",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/sample/subcmd-demo",
            "args": [
                "migrate",
            ]
        }
    ]
}
